<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="WebDb" connectionString="Server=localhost;Database=WebDb;Integrated Security=SSPI;Connect Timeout=300"
         xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="Master" connectionString="Server=localhost;Database=master;Integrated Security=SSPI;Connect Timeout=300"
         xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
</configuration>